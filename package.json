{
  "name": "dwv-vue",
  "version": "0.11.0-beta.0",
  "description": "Medical viewer using DWV (DICOM Web Viewer) and Vue.js.",
  "author": "ivmartel",
  "license": "GPL-3.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-trunk": "vite build --mode development --base /dwv-vue/demo/trunk",
    "build-stable": "vite build --mode production --base /dwv-vue/demo/stable",
    "preview": "vite preview",
    "test": "vitest",
    "test-ci": "vitest run --coverage",
    "lint": "eslint src/**/*.{js,vue}"
  },
  "dependencies": {
    "dwv": "0.36.1",
    "vue": "^3.5.28"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "canvas": "^3.2.1",
    "eslint": "^10.0.0",
    "eslint-plugin-vue": "^10.8.0",
    "jsdom": "^28.1.0",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.18",
    "vue-eslint-parser": "^10.4.0",
    "vuetify": "^3.11.8"
  },
  "packageManager": "yarn@4.12.0"
}
